function random() do
    return call("Srandom", "C:\\Suny\\libs\\random\\random.dll", [])
end

random()

function randint(a, b) do
    if a > b then
        tmp = a
        a = b
        b = tmp
    end
    return a + int(random() * (b - a + 1))
end

function choice(l) do
    n = size(l) - 1
    i = 0

    return l[randint(n, i)]
end