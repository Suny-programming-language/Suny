function tokenize(buff) do
    n = size(buff)
    i = 0
    tokens = []

    while i < n do
        if buff[i] == "+" or buff[i] == "-" then
            push(tokens, buff[i])
            i += 1
            continue
        end

        if isdigit(buff[i]) then
            num = ""
            while isdigit(buff[i]) and i < n do
                num += buff[i]
                i += 1
            end
            push(tokens, num)
            continue
        end

        i += 1
    end

    return tokens
end

function eval(tokens) do
    i = 1
    n = size(tokens)
    result = number(tokens[0])

    while i < n do
        if tokens[i] == "+" then
            result += number(tokens[i + 1])
            i += 2
            continue
        end

        if tokens[i] == "-" then
            result -= number(tokens[i + 1])
            i += 2
            continue
        end
    end

    return result
end 
